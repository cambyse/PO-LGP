  pages = {173--182},
  keywords = {Active learning,applications,medicine,text classification},
  file = {ACM Full Text PDF:/home/johannes/.zotero/zotero/55kac6rx.default/zotero/storage/KUABS9HE/Wallace et al. - 2010 - Active Learning for Biomedical Citation Screening.pdf:application/pdf}
