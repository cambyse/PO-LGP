  number = {2},
  journaltitle = {PLOS ONE},
  shortjournal = {PLOS ONE},
  author = {Dugas, Andrea Freyer and Jalalpour, Mehdi and Gel, Yulia and Levin, Scott and Torcaso, Fred and Igusa, Takeru and Rothman, Richard E.},
  urldate = {2016-03-04},
  date = {2013-02-14},
  pages = {e56176},
  keywords = {Critical care and emergency medicine,Forecasting,Generalized linear model,Humidity,Infectious disease surveillance,Influenza,Influenza A virus,Seasons},
  file = {Full Text PDF:/home/johannes/.zotero/zotero/55kac6rx.default/zotero/storage/G9NT7BJC/Dugas et al. - 2013 - Influenza Forecasting with Google Flu Trends.pdf:application/pdf;Snapshot:/home/johannes/.zotero/zotero/55kac6rx.default/zotero/storage/Z5KQGRAA/article.html:text/html}
