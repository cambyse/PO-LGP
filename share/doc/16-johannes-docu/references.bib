@inproceedings{dima_enabling_2004,
  title = {Enabling learning from large datasets: applying active learning to mobile robotics},
  volume = {1},
  shorttitle = {Enabling learning from large datasets},
  abstract = {Autonomous navigation in outdoor, off-road environments requires solving complex classification problems. Obstacle detection, road following and terrain classification are examples of tasks which have been successfully approached using supervised machine learning techniques for classification. Large amounts of training data are usually necessary in order to achieve satisfactory generalization. In such cases, manually labeling data becomes an expensive and tedious process. This work describes a method for reducing the amount of data that needs to be presented to a human trainer. The algorithm relies on kernel density estimation in order to identify "interesting" scenes in a dataset. Our method does not require any interaction with a human expert for selecting the images, and only minimal amounts of tuning are necessary. We demonstrate its effectiveness in several experiments using data collected with two different vehicles. We first show that our method automatically selects those scenes from a large dataset that a person would consider "important" for classification tasks. Secondly, we show that by labeling only few of the images selected by our method, we obtain classification performance that is comparable to the one reached after labeling hundreds of images from the same dataset.},
  eventtitle = {2004 IEEE International Conference on Robotics and Automation, 2004. Proceedings. ICRA '04},
  timestamp = {2016-03-24T10:31:31Z},
  booktitle = ICRA,
