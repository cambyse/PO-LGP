  file = {Full Text PDF:/home/johannes/.zotero/zotero/55kac6rx.default/zotero/storage/XGAFT5E6/Jonschkowski and Brock - 2015 - Learning state representations with robotic priors.pdf:application/pdf;Snapshot:/home/johannes/.zotero/zotero/55kac6rx.default/zotero/storage/F9Z3XTIZ/s10514-015-9459-7.html:text/html}
