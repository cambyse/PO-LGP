Symbol conv
Symbol timeout
Symbol contact
Symbol quit

start
CoreTasks
FollowReferenceActivity
HomingActivity

endeffHead
endeffL
gazeAtHand (FollowReferenceActivity){ type="gazeAt", PD=[.5 .9 .1 10.], prec=1. }
alignHand (FollowReferenceActivity){ type="vec", vec1=[1 0 0], target=[0.7071, 0, -0.7071], PD=[.5, .9, .1, 10.] }
positionHand (FollowReferenceActivity){ type="pos", target=[.7, .3, .7], PD=[.5, .9, .1, 10.] }
lowerHand (FollowReferenceActivity){ type="pos", ref1="endeffL", target=[.7, .3, .49], PD=[.5, .9, .1, 10.] }
controlForce (FollowReferenceActivity){ type="forceCtrl", ref1="endeffForceL", target=[0 0 -7], timeOut=5. }
homing (HomingActivity){ type="homing" }

STATE { (CoreTasks) (gazeAtHand endeffHead endeffL) (alignHand endeffL) (positionHand endeffL) }

Rule {
  { (conv positionHand) (conv alignHand) }
  { (lowerHand) (positionHand)! (conv positionHand)! }
}

Rule {
  { (lowerHand) (conv lowerHand) }
  { (homing) (lowerHand)! (conv lowerHand)! (gazeAtHand)! (alignHand)! (conv alignHand)! }
}

Rule {
  { (homing) (conv homing) }
  { (quit) (homing)! (conv homing)! }
}

Script {
  (gazeAtHand endeffHead endeffL)
  (alignHand endeffL)
  (positionHand endeffL)
  { (conv alignHand) (conv positionHand) }
  (lowerHand)
}
