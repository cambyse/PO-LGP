<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="window">
    <property name="height_request">200</property>
    <signal name="destroy" handler="on_window_destroy" after="yes"/>
